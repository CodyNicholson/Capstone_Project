@model Capstone_Project.Models.Survey

@{
    ViewBag.Title = "My Results";
    ViewBag.Message = "This is the page where you can view your results compared against everyone else who has taken the survey.";
}
<h2>@ViewBag.Title</h2>
<p>@ViewBag.Message</p>

<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">

        <style type="text/css">
            circle.node {
                cursor: pointer;
                stroke: #000;
                stroke-width: .5px;
            }

            line.link {
                fill: none;
                stroke: #9ecae1;
                stroke-width: 1.5px;
            }
        </style>
    </head>
    <body>
        <script src="http://d3js.org/d3.v3.min.js" charset="utf-8"></script>
        <script src="~/Scripts/libraries/d3.geom.js"></script>
        <script src="~/Scripts/libraries/d3.layout.js"></script>
        <script src="~/Scripts/sketch.js"></script>
    </body>
</html>